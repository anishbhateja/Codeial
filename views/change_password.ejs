<h1>Update Password</h1>
<form action="/reset-password/change-password/<%=accessToken%>" method="POST">
  <input
    type="password"
    name="password1"
    placeholder="Enter Password"
    required
  />
  <input
    type="password"
    name="password2"
    placeholder="Confirm Password"
    required
  />
  <input type="submit" value="Submit" />
</form>
